{
  "name": "@dee-soft/ginie-micro",
  "version": "1.0.2",
  "description": "CLI tool to generate microservice monorepo structures with REST or gRPC communication",
  "main": "bin/ginie.js",
  "bin": {
    "ginie-micro": "./bin/ginie.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "start": "node bin/ginie.js",
    "commit": "git-cz",
    "test": "jest",
    "lint": "eslint .",
    "audit": "npm audit --audit-level=moderate",
    "security-check": "node utils/security.js",
    "prepare": "husky install",
    "update:docker": "node scripts/update-versions.js docker",
    "update:npm": "node scripts/update-versions.js npm",
    "update:all": "node scripts/update-versions.js all",
    "check:versions": "node -e \"require('./utils/version-checker').checkAllVersions().then(console.log)\"",
    "ginie": "npx ginie-micro",
    "dev": "concurrently \"npm run dev --workspace=*\""
  },
  "keywords": [
    "microservice",
    "generator",
    "cli",
    "monorepo",
    "rest",
    "grpc",
    "nodejs"
  ],
  "author": "Dee-Soft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dee-Soft/Ginie-micro/issues"
  },
  "homepage": "https://github.com/Dee-Soft/Ginie-micro#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.5.0",
    "fs-extra": "^11.3.1",
    "inquirer": "^8.2.7",
    "js-yaml": "^4.1.0",
    "simple-git": "^3.28.0",
    "axios": "^1.5.0",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.48.0",
    "husky": "^9.1.7",
    "jest": "^29.6.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
